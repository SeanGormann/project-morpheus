{
  "expo": {
    "name": "SleepSounds",
    "slug": "sleepsounds",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sleepsounds.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0f172a"
      },
      "package": "com.sleepsounds.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "react-native-health"
    ]
  }
}
